enium\n","pip install selenium\n","pip install webdriver_manager\n"]},{"cell_type":"code","source":["from webdriver_manager.chrome import ChromeDriverManager\n","from selenium import webdriver\n","from selenium.webdriver.chrome.service import Service\n","from selenium.webdriver.common.by import By\n","from selenium.webdriver.common.action_chains import ActionChains\n","from selenium.webdriver.support.wait import WebDriverWait\n","from selenium.webdriver.support import expected_conditions as EC\n","\n","chrome_options = webdriver.ChromeOptions()\n","chrome_options.add_argument('--headless')\n","chrome_options.add_argument(\"window-size=1920,1080\")\n","chrome_options.add_argument('--no-sandbox')\n","chrome_options.add_argument('--disable-dev-shm-usage')\n","\n","service = Service(executable_path='/usr/bin/chromedriver')\n","\n","driver = webdriver.Chrome(service=service, options=chrome_options)\n","\n","time_out = 10\n","\n","try:\n","    # Отправим браузер на страницу\n","    driver.get(url=\"https://www.nbcomputers.ru/\")\n","    driver.implicitly_wait(time_out)\n","\n","    driver.switch_to.active_element\n","    css_input = '.ant-input.ant-input-sm.digi-instant-search.jc-ignore'\n","    input_field = driver.find_element(by=By.CSS_SELECTOR, value=css_input)\n","    input_field.click()\n","\n","    input_field.send_keys('lenovo\\n')\n","    driver.implicitly_wait(time_out)\n","\n","    button_search = driver.find_element(by=By.CSS_SELECTOR, value='.ant-btn.ant-btn-link.sc-381c052-1.clahoV')\n","    actions = ActionChains(driver)\n","    driver.switch_to.active_element\n","    actions.move_to_element(button_search).click().perform()\n","\n","    len_list = 0\n","    while True:\n","      els = driver.find_elements(by=By.CSS_SELECTOR,value='div.digi-product')\n","      if len(els) > len_list:\n","        len_list = len(els)\n","        ActionChains(driver).scroll_to_element(els[-1]).perform()\n","        actions.move_to_element(els[-1]).perform()\n","        print('---->', len(els))\n","      else:\n","        break\n","\n","\n","except Exception as ex:\n","    print(f'ERROR - {ex}')\n","\n","finally:\n","    html = driver.page_source\n","    driver.quit()\n","    print('THE END')"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"blfikCGl0AHI","executionInfo":{"status":"ok","timestamp":1683808798049,"user_tz":-240,"elapsed":20802,"user":{"displayName":"S.V. S","userId":"09304436901716683394"}},"outputId":"15178305-21a4-4137-844b-56953c81d6af"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["----> 20\n","----> 40\n","----> 60\n","----> 80\n","----> 100\n","----> 107\n","THE END\n"]}]},{"cell_type":"code","source":["from bs4 import BeautifulSoup\n","\n","soup = BeautifulSoup(html, 'html.parser')\n","block = soup.find_all(\"a\", attrs={\"class\": \"digi-product__label\"})\n","\n","print(len(block))\n","for a in block:\n","  print(a.text)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"Avvfwbcg01cK","executionInfo":{"status":"ok","timestamp":1683808820869,"user_tz":-240,"elapsed":410,"user":{"displayName":"S.V. S","userId":"09304436901716683394"}},"outputId":"9f36698e-e9ec-470e-b7b5-ed26c8e3de45"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["107\n","\n","          Ноутбук Lenovo Legion 5 15IAH7H 15.6″/16/SSD 1024/серый\n","        \n","\n","          Ноутбук Lenovo Legion 5 15IAH7H 15.6″/32/SSD 1024/серый\n","        \n","\n","          Ноутбук Lenovo Legion 5 Pro 16IAH7H 16″/16/SSD 1024/белый\n","        \n","\n","          Ноутбук Lenovo Legion 7 16ACHG6 16″/16/SSD 1024/серый\n","        \n","\n","          Ноутбук Lenovo ThinkBook 16p G2 ACH 16″/16/SSD 1024/серый\n","        \n","\n","          Ноутбук Lenovo Legion 5 Pro 16IAH7H 16″/16/SSD 1024/серый\n","        \n","\n","          Ноутбук Lenovo IdeaPad Gaming 3 15ACH6 15.6″/8/SSD 512/черный\n","        \n","\n","          Ноутбук Lenovo Legion 5 Pro 16IAH7H 16″/32/SSD 